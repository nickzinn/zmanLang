func add(a, b) {
  return a + b;
}

func twice(s) {
  return s + "" + s;
}

func inc2(x) {
  let y := x + 1;
  y := y + 1;
  return y;
}

func fact(n) {
  if (n = 0) {
    return 1;
  } else {
    return n * fact(n - 1);
  }
}

println(text(add(2, 3)));
println(twice("ha"));
println(text(inc2(40)));
println(text(fact(5)));

add(1, 2);
