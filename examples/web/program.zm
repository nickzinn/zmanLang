print("Hello world!\n");
print("Lets play with arrays!\n");

let data := [5];
let i := 0;
while (i < length(data)) {
    data[i] := i;
    i := i + 1;
}

print(data);
print("\n");

func sum(array[]) {
  let s := 0;
  foreach(v, array) {
    s := s + v;
  }
  return s;
}

print("The sum is " + text(sum(data)) + "\n");
