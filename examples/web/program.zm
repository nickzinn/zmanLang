println("Hello world!");
println("Lets play with arrays!");

let data := [5];
let i := 0;
while (i < length(data)) {
    data[i] := i;
    i := i + 1;
}

println(data);

func sum(array[]) {
  let s := 0;
  foreach(v, array) {
    s := s + v;
  }
  return s;
}

println("The sum is " + text(sum(data)));
