func printArray(array[]) {
  print("[");
  let first := true;
  let j :=0;
  while(j < length(array)) {
    if (first) {
      first := false;
    } else {
      print(", ");
    }
    print(text(array[j]));
    j := j+1;
  }
  print("]");
  return 0;
}

